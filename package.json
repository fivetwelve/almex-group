{
  "name": "almex-group-poc",
  "description": "Starter kit for new Almex Group site",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop -H $(hostname) -p 8000",
    "start": "gatsby develop -p 8000",
    "start:host": "gatsby develop  -H $(hostname) -p 8000",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "build": "echo SITE_RECAPTCHA_KEY=$SITE_RECAPTCHA_KEY >> .env.production && gatsby build && cp _redirects public/",
    "build:content": "echo SITE_RECAPTCHA_KEY=$SITE_RECAPTCHA_KEY >> .env.production && gatsby build && cp _redirects--content public/",
    "serve": "gatsby serve",
    "start:lambda": "netlify-lambda serve src/lambda -c webpack.config.js",
    "build:lambda": "netlify-lambda build src/lambda -c webpack.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.6",
    "@babel/polyfill": "^7.2.5",
    "@reach/router": "^1.2.1",
    "@react-google-maps/api": "^1.3.0",
    "@turf/circle": "^6.0.1",
    "algoliasearch": "^3.35.1",
    "browser-monads": "^1.0.0",
    "dotenv": "^8.0.0",
    "gatsby": "^2.23.3",
    "gatsby-plugin-google-analytics": "^2.0.9",
    "gatsby-plugin-netlify": "^2.3.5",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet-async": "^1.0.14",
    "gatsby-plugin-remote-images": "^1.0.1",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-plugin-typography": "^2.5.4",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-source-graphql": "^2.5.4",
    "gatsby-transformer-remark": "^2.8.15",
    "graphcms-image": "^1.1.0-beta4",
    "graphql-tag": "^2.10.3",
    "gsap": "^2.1.2",
    "http-proxy-middleware": "^0.20.0",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^2.3.0",
    "node-loggly-bulk": "^2.2.4",
    "node-mailjet": "^3.3.1",
    "node-sass": "^4.11.0",
    "nuka-carousel": "^4.5.13",
    "object-fit-images": "^3.2.4",
    "prop-types": "^15.6.2",
    "qs": "^6.9.0",
    "react": "^16.12.0",
    "react-cookie-consent": "^3.0.0",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.12.0",
    "react-google-recaptcha": "^2.0.1",
    "react-helmet-async": "^1.0.4",
    "react-icons": "^3.3.0",
    "react-instantsearch-dom": "^6.2.0",
    "react-markdown": "^4.3.1",
    "react-moment-proptypes": "^1.6.0",
    "react-typography": "^0.16.18",
    "react-waypoint": "^9.0.2",
    "react-youtube": "^7.9.0",
    "typography": "^0.16.18",
    "webpack": "^4.33.0"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^3.0.9",
    "prettier": "^1.15.3"
  },
  "version": "0.0.1"
}
