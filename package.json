{
  "name": "almex-group-poc",
  "description": "Starter kit for new Almex Group site",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop -H $(hostname) -p 8000",
    "start": "gatsby develop -p 8000",
    "start:host": "gatsby develop  -H $(hostname) -p 8000",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "build": "echo SITE_RECAPTCHA_KEY=$SITE_RECAPTCHA_KEY >> .env.production && GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages && cp _redirects public/",
    "build:content": "echo SITE_RECAPTCHA_KEY=$SITE_RECAPTCHA_KEY >> .env.production && GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages && cp _redirects--content public/_redirects",
    "serve": "gatsby serve",
    "start:lambda": "netlify-lambda serve src/lambda -c webpack.config.js",
    "build:lambda": "netlify-lambda build src/lambda -c webpack.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@babel/polyfill": "^7.2.5",
    "@reach/router": "^1.2.1",
    "@reach/skip-nav": "^0.15.0",
    "@react-google-maps/api": "^1.3.0",
    "@turf/circle": "^6.0.1",
    "algoliasearch": "^3.35.1",
    "browser-monads": "^1.0.0",
    "cross-fetch": "^3.1.4",
    "dotenv": "^8.0.0",
    "encoding": "^0.1.13",
    "gatsby": "^3.5.1",
    "gatsby-link": "^3.5.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^3.5.0",
    "gatsby-plugin-netlify": "^3.5.0",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-react-helmet-async": "^1.2.0",
    "gatsby-plugin-remote-images": "^3.0.0",
    "gatsby-plugin-robots-txt": "1.5.6",
    "gatsby-plugin-sass": "^4.5.0",
    "gatsby-plugin-typography": "^3.5.0",
    "gatsby-react-router-scroll": "^4.5.0",
    "gatsby-source-filesystem": "^3.5.0",
    "gatsby-source-graphql": "^3.5.0",
    "gatsby-transformer-remark": "^4.2.0",
    "graphcms-image": "^1.1.0-beta4",
    "graphql-tag": "^2.12.4",
    "gsap": "^2.1.2",
    "http-proxy-middleware": "^0.20.0",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.29.1",
    "netlify-lambda": "^2.0.7",
    "node-mailjet": "^3.3.1",
    "node-sass": "^4.11.0",
    "nuka-carousel": "^4.5.13",
    "object-fit-images": "^3.2.4",
    "path-browserify": "^1.0.1",
    "prop-types": "^15.6.2",
    "qs": "^6.9.0",
    "react": "^16.12.0",
    "react-cookie-consent": "^3.0.0",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.12.0",
    "react-google-recaptcha": "^2.0.1",
    "react-helmet-async": "^1.0.4",
    "react-icons": "^3.3.0",
    "react-instantsearch-dom": "^6.2.0",
    "react-markdown": "^6.0.2",
    "react-moment-proptypes": "^1.6.0",
    "react-typography": "^0.16.18",
    "react-waypoint": "^9.0.2",
    "react-youtube": "^7.9.0",
    "rehype-raw": "^5.1.0",
    "remark-gfm": "^1.0.0",
    "typography": "^0.16.18",
    "webpack": "^5.37.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^3.0.9",
    "prettier": "^1.15.3"
  },
  "resolutions": {
    "graphql": "15.x",
    "webpack": "^5.0.0"
  },
  "version": "0.0.1"
}
